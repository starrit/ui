{
  "name": "web-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --hot --config ./config/webpack.config.dev.js",
    "build": "webpack --config ./config/webpack.config.prod.js",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "devDependencies": {
    "@storybook/cli": "^3.3.15",
    "@storybook/react": "^3.4.8",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.5.0",
    "dotenv-webpack": "^1.5.5",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.10.4",
    "node-sass": "^4.9.2",
    "npm": "^5.7.1",
    "postcss-loader": "^2.1.1",
    "sass-loader": "^6.0.7",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^1.0.1",
    "webpack": "3.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^2.1.0",
    "webpack-merge": "^3.0.0"
  },
  "dependencies": {
    "@gstarrltd/fragmented": "^0.0.13",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "auth0-js": "^9.6.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "classname": "0.0.0",
    "core-js": "^2.5.5",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "fetch-jsonp": "^1.1.3",
    "fetch-mock": "^6.3.0",
    "jest": "^22.4.3",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.9",
    "react-form-with-constraints": "^0.8.0",
    "react-player": "^1.6.4",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.1.41",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.1",
    "react-validation": "^3.0.7",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "resolve-url-loader": "^2.3.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.81.1",
    "storybook": "^1.0.0",
    "style-loader": "^0.21.0",
    "url-search-params-polyfill": "^3.0.0",
    "whatwg-fetch": "^2.0.4",
    "xhr-mock": "^2.3.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/constants/"
    ],
    "moduleFileExtensions": [
      "js",
      "scss"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "AppActions(.*)$": "<rootDir>/src/app/actions/$1",
      "AppComponents(.*)$": "<rootDir>/src/app/components/$1",
      "AppReducers(.*)$": "<rootDir>/src/app/reducers/$1",
      "AppServices(.*)$": "<rootDir>/src/app/services/$1",
      "AppUtils(.*)$": "<rootDir>/src/app/utils/$1",
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    }
  }
}
